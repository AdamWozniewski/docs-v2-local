{
  "name": "@auth0/docs-ui",
  "version": "1.0.0",
  "description": "Custom UI components for Auth0 Documentation",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "version:patch": "pnpm version patch && echo 'Updated to patch version'",
    "version:minor": "pnpm version minor && echo 'Updated to minor version'",
    "version:major": "pnpm version major && echo 'Updated to major version'",
    "version:current": "node scripts/update-main-version.js --current",
    "version:update-main": "node scripts/update-main-version.js",
    "release": "pnpm run build && pnpm run version:update-main && echo 'Ready for deployment!'"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-links": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^9.1.7",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vite": "^7.1.6"
  }
}
